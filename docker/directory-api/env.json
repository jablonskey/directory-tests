{
  "host_env_var_prefix": "DIRECTORY_API",
  "file_path": "./docker/directory-api/.env",
  "env_vars": {
    "required": [
      "UI_SECRET",
      "SECRET_KEY",
      "DATABASE_URL",
      "AWS_ACCESS_KEY_ID",
      "AWS_SECRET_ACCESS_KEY",
      "LOCUST_NUM_REQUEST",
      "LOCUST_NUM_CLIENTS",
      "LOCUST_HATCH_RATE",
      "DIRECTORY_UI_LOAD_URL"
    ],
    "optional": [
      "PORT",
      "DEBUG",
      "SQS_REGION_NAME",
      "SQS_ENROLMENT_QUEUE_NAME",
      "SQS_INVALID_ENROLMENT_QUEUE_NAME",
      "SQS_WAIT_TIME",
      "SQS_MAX_NUMBER_OF_MESSAGES",
      "SQS_VISIBILITY_TIMEOUT",
      "LOCUST_MIN_WAIT",
      "LOCUST_MAX_WAIT"
    ]
  }
}
