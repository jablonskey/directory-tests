{
  "host_env_var_prefix": "DIRECTORY_TESTS",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
        "DIRECTORY_API_URL",
        "DIRECTORY_SSO_URL",
        "DIRECTORY_UI_BUYER_URL",
        "DIRECTORY_UI_SUPPLIER_URL",
        "LOCUST_HATCH_RATE",
        "LOCUST_NUM_CLIENTS",
        "SSO_USER_USERNAME",
        "SSO_USER_PASSWORD"
    ],
    "optional": [
        "LOCUST_MAX_WAIT",
        "LOCUST_MIN_WAIT",
        "LOCUST_TIMEOUT"
    ]
  }
}
