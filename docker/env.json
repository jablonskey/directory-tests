{
  "host_env_var_prefix": "DIRECTORY_TESTS",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
        "API_CLIENT_KEY",
        "DIRECTORY_API_URL",
        "DIRECTORY_SSO_URL",
        "DIRECTORY_BUYER_UI_URL",
        "DIRECTORY_SUPPLIER_UI_URL",
        "LOCUST_HATCH_RATE",
        "LOCUST_NUM_CLIENTS"
    ],
    "optional": [
        "LOCUST_MAX_WAIT",
        "LOCUST_MIN_WAIT",
        "LOCUST_TIMEOUT"
    ]
  }
}
