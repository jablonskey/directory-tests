{
  "host_env_var_prefix": "DIRECTORY_TESTS",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
        "DIRECTORY_API_URL",
        "DIRECTORY_BUYER_API_URL",
        "DIRECTORY_SSO_URL",
        "DIRECTORY_UI_BUYER_URL",
        "DIRECTORY_UI_SUPPLIER_URL",
        "DIRECTORY_PROFILE_URL",
        "LOCUST_HATCH_RATE",
        "LOCUST_NUM_CLIENTS",
        "MAILGUN_DOMAIN",
        "MAILGUN_SECRET_API_KEY",
        "SSO_USER_USERNAME",
        "SSO_USER_PASSWORD",
        "SSO_USER_TOKEN",
        "SSO_UNVERIFIED_USER_TOKEN",
        "SSO_USER_SSO_ID",
        "HEROKU_API_KEY"
    ],
    "optional": [
        "LOCUST_MAX_WAIT",
        "LOCUST_MIN_WAIT",
        "LOCUST_TIMEOUT",
        "SSO_COMPANY_NOT_ACTIVE",
        "SSO_COMPANY_ACTIVE_NOT_REGISTERED",
        "SSO_COMPANY_ALREADY_REGISTERED"
    ]
  }
}
